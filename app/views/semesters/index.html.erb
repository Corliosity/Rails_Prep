<h1>Listing semesters</h1>

<table>
  <tr>
    <th>Course</th>
    <th>Student</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @semesters.each do |semester| %>
  <tr>
    <td><%= semester.course_id %></td>
    <td><%= semester.student_id %></td>
    <td><%= link_to 'Show', semester %></td>
    <td><%= link_to 'Edit', edit_semester_path(semester) %></td>
    <td><%= link_to 'Destroy', semester, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Semester', new_semester_path %>
